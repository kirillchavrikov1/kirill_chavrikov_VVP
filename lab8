#include <stdio.h> 
int main()
{
    int R;
    printf("R:");
    scanf ("%d", &R);
    if (R%1024>0)
        printf("Килобайт: %d\n",R/1024+1);
    else
        printf("Килобайт: %d\n",R/1024+1);
}
