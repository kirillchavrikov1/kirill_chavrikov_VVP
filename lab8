3.
#include <stdio.h>
int main()
{
  int a,b,c;
    printf("Введите A: ");
    scanf ("%d", &a);
    printf("Введите B: ");
    scanf ("%d", &b);
      if (a>b)
       {
           c=a%b;
        printf("Длинна незанятой части: %d", c);
       }
        else 
       {
        printf("Ведённые данные неверны");
       }
}
