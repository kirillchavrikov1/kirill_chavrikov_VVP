2.
#include <stdio.h>
int main()
{
    int A, B;
      printf("Введите A:");
      scanf ("%d", &A);
      printf("Введите B:");
      scanf ("%d", &B);
        if (A>B)
         {
           printf("Ответ: %d\n",A/B);
         }
        else 
         {
           printf("Ведённые данные неверны");
         }
}
