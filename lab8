5.
#include <stdio.h> 
int main()
{
    int C,z,x,y;
    printf("C:");
    scanf ("%d", &C);
    z = C / 100;
    x = (C / 10) % 10;
    y = C % 10;
    printf("%d\n", (x * 100) + (y * 10) + z);
}
