2. 
#include <stdio.h>
 int main()
{
    int k;
      printf("Введите  K:");
       scanf ("%d", &k);
      printf("Ответ: %d\n",k%7+1);
}
==============================================
3. 
#include <stdio.h>
int main()
{
    int K,N;
    printf("Введите K:");
    scanf ("%i", &K);
    printf("Введите N:");
    scanf ("%i", &N);
    printf("%i\n",((K+N-2)%7)+1);
}
4. 
#include <stdio.h>
int main()
{
    int a,b,c;
    printf("Введите  A:");
    scanf ("%d", &a);
    printf("Введите  B:");
    scanf ("%d", &b);
    printf("Введите  C:");
    scanf ("%d", &c);
    printf("Ответ: %d\n",(a/c)*(b/c));
    printf("Ответ(оставшаяся площадь): %d\n",(a*b)-((a/c)*(b/c)*(c*c)));
}
==================================================
5. 
	#include <stdio.h> 
int main()
{
    int g;
    printf("Введите  год:");
    scanf ("%d", &g);
    printf("Столетие: %d\n",((g-1)/100)+1);
 }
